{
  "name": "react-modular-ui",
  "version": "0.13.0",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/codetraceio/react-modular-ui"
  },
  "devDependencies": {
    "@types/express": "^4.0.36",
    "@types/node": "^8.0.10",
    "@types/react": "^15.0.35",
    "coveralls": "^2.13.0",
    "express": "^4.15.2",
    "jest": "^19.0.2",
    "react-test-renderer": "^15.5.4",
    "stylus": "^0.54.5",
    "ts-loader": "^2.2.2",
    "typescript": "^2.4.1",
    "webpack": "^2.4.1"
  },
  "dependencies": {
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "scripts": {
    "start": "node dist/example/server/index.js",
    "test": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test-dev": "jest --coverage",
    "build": "./run/build.sh"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/components/**/*.tsx",
      "src/settings.ts",
      "src/index.ts",
      "src/utils.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx|js)$": "./preprocessor.js"
    },
    "testMatch": [
      "**/tests/**/*.(ts|tsx|js)"
    ]
  }
}
